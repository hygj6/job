<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/fmt" %><!DOCTYPE html><html>    <head>        <meta charset="UTF-8">        <title>面试日程 - 外贸帮手网</title>        <jsp:include page="/WEB-INF/public/public-icon.jsp"/>    </head>    <body>        <jsp:include page="/WEB-INF/public/public-company-head.jsp"/>        <div class="company-cont wrap-1200">            <div class="middle-cont">                <div class="race-selection">                    <form id="schedule-form">                    <dl>                        <dt>求职者：</dt>                        <dd><input value="${q}" placeholder="求职者的姓名 / 电话" name="q" type="text" class="input-text"/></dd>                        <dt>职位：</dt>                        <dd style="padding-right: 20px;">                            <select name="category_id" class="input-text select-custom">                                <option value="">不限</option>                                <c:forEach items="${categories}" var="category">                                <option value="${category.id}" <c:if test="${category.id == category_id}">selected</c:if>>${category.job}</option>                                </c:forEach>                            </select>                        </dd>                        <dd>                            <button class="btn" type="submit">提交</button>                        </dd>                    </dl>                    </form>                </div>                <div class="blank-2"></div>                <div class="nav-tab">                    <ul id="schedule-tab">                        <li <c:if test="${schedule == 0}">class="on"</c:if>><a data-id="0" href="javascript:void(0)">不限</a></li>                        <li <c:if test="${schedule == 1}">class="on"</c:if>><a data-id="1" href="javascript:void(0)">以前</a></li>                        <li <c:if test="${schedule == 2}">class="on"</c:if>><a data-id="2" href="javascript:void(0)">今天</a></li>                        <li <c:if test="${schedule == 3}">class="on"</c:if>><a data-id="3" href="javascript:void(0)">以后</a></li>                    </ul>                </div>                <div class="blank-2"></div>                <c:if test="${count>0}">                <table class="table">                    <tr>                        <th>面试时间</th>                        <th>求职者</th>                        <th>电话</th>                        <th>岗位</th>                    </tr>                    <c:forEach items="${schedules}" var="data">                        <tr>                            <td><fn:formatDate value="${data.scheduleDate}" pattern="yyyy/MM/dd E HH:mm"/></td>                            <td><span class="sign">${data.name}</span></td>                            <td>${data.mobile}</td>                            <td>${data.jobName}</td>                        </tr>                    </c:forEach>                </table>                </c:if>                <c:if test="${count==0}">                <jsp:include page="/WEB-INF/public/public-unusual-state.jsp" flush="true">                    <jsp:param name="top" value="100px"/>                    <jsp:param name="msg" value="没有任何面试安排<br>如果你还没有发布职位，需要先发布职位"/>                </jsp:include>                </c:if>                <c:if test="${count>0}">                <div style="padding-top: 50px;" id="pagination"></div>                </c:if>            </div>        </div>        <jsp:include page="/WEB-INF/public/public-foot.jsp"/>        <jsp:include page="/WEB-INF/public/public-js.jsp"/>        <jsp:include page="/WEB-INF/public/public-page.jsp" flush="true">            <jsp:param name="total" value="15"/>            <jsp:param name="size" value="15"/>        </jsp:include>    </body></html>